<!DOCTYPE html>
<html>
<head>
    <style>
        h2, h4 {
            padding: 0px;
            margin: 0px;
        }

        h2 {
            font-weight: bold;
            font-size: 16px;
            color: green;
            padding-left: 20px;
        }

        h4 {
            font-weight: bold;
            font-size: 12px;
            color: blue;
            padding-left: 40px;
        }
        .user-line {
            padding-left: 60px;
        }
    </style>
</head>
<body>
<h1>用户列表</h1>
<div>
    <button id="refresh_data_button">刷新数据</button>
</div>
<div>
    <% for(var i = 0;i < data.length;i++){
        var ipSegment = data[i]; %>
    <h2> <%= ipSegment.ip %></h2>
    <% for(var m = 0;m < ipSegment.users;m++){ %>
    <% var user = ipSegment.users[m]; %>
    <h4><%= user.role %></h4>
    <% } %>
    <% } %>
</div>
</body>
</html>
<script src="/javascripts/jquery.js"></script>
<script src="/javascripts/pages/home.js"></script>
